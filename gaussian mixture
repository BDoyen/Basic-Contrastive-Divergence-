%génération des training examples
d = 1;
mu = [20;50];
k = [5 5];
sigma = zeros(1,d,length(k));
for i = 1:length(k)
        sigma(:,:,i)=k(i);
end;

%poids des gaussiennes
p = [1 2];

%génération de l’objet gmdistribution
obj = gmdistribution(mu,sigma,p);
Y = random(obj,m);

%Densité théorique
Xnormal = pdf(obj,transpose(X)) ;
